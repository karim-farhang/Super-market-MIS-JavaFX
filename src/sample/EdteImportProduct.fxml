<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<HBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" onMouseDragged="#MouseDeagged" onMousePressed="#MousePressed" prefHeight="490.0"
      prefWidth="278.0" xmlns="http://javafx.com/javafx/11.0.1"
      fx:controller="sample.EditeImportProduct">
    <children>
        <VBox alignment="BOTTOM_CENTER" onMouseDragged="#Mouse_Dragged" onMousePressed="#Mouse_pressed"
              prefHeight="400.0" prefWidth="216.0" spacing="30.0">
            <children>
                <JFXTextField fx:id="txt_name" alignment="CENTER" labelFloat="true" prefHeight="30.0" prefWidth="220.0"
                              promptText="نام جنس را وارید کنید"/>
                <JFXComboBox fx:id="comb_catagory" labelFloat="true" nodeOrientation="RIGHT_TO_LEFT" prefHeight="30.0"
                             prefWidth="220.0" promptText="          نوعیت جنس را انتخاب کنید"/>
                <JFXTextField fx:id="txt_total" alignment="CENTER" labelFloat="true" prefHeight="30.0" prefWidth="220.0"
                              promptText="تعداد جنس را وارید کنید"/>
                <JFXComboBox fx:id="comb_unit" labelFloat="true" nodeOrientation="RIGHT_TO_LEFT" prefHeight="30.0"
                             prefWidth="220.0" promptText="          واحد جنس را انتخاب کنید"/>
                <JFXTextField fx:id="txt_price" alignment="CENTER" labelFloat="true" prefHeight="30.0" prefWidth="220.0"
                              promptText="قیمت جنس را وارید کنید"/>
                <JFXTextField fx:id="txt_import_date" alignment="CENTER" labelFloat="true" prefHeight="30.0"
                              prefWidth="200.0" promptText="تاریخ ثبت"/>
                <JFXTextField fx:id="txt_expire_date" alignment="CENTER" labelFloat="true" prefHeight="30.0"
                              prefWidth="200.0" promptText="تاریخ انقضآ"/>
                <JFXComboBox fx:id="comb_sell_name" labelFloat="true" nodeOrientation="RIGHT_TO_LEFT" prefHeight="30.0"
                             prefWidth="220.0" promptText="         اسم فروشنده را انتخاب کنید"/>
                <HBox alignment="CENTER" prefHeight="41.0" prefWidth="216.0" spacing="10.0">
                    <children>
                        <JFXButton fx:id="btn_close" buttonType="RAISED" mnemonicParsing="false"
                                   onAction="#btn_close_onAction" prefHeight="30.0" prefWidth="90.0"
                                   style="-fx-background-color: rgb(200,20,20);" text="بستن" textFill="#eeecec"/>
                        <JFXButton fx:id="btn_ok" buttonType="RAISED" mnemonicParsing="false"
                                   onAction="#btn_on_onAction" prefHeight="30.0" prefWidth="90.0"
                                   style="-fx-background-color: rgb(20,200,20);" text="ثبت" textFill="#eeecec"/>
                    </children>
                </HBox>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</HBox>
